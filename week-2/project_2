fn main() {
	let t:f64 = 450000.0;
	let m:f64 = 1500000.0;
	let h:f64 = 750000.0;
	let d:f64 = 2850000.0;
	let a:f64 = 250000.0;

	let sum = (t*2.0) + (m*1.0) + (h*3.0) + (d*3.0) + (a*1.0);
	println!("The Sum of the record is {}", sum);
	let avg = sum/(2.0+1.0+3.0+3.0+1.0);
	println!("The Average of the record is {}", avg)

}